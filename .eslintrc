{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
  },
  "plugins": ["@typescript-eslint","prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended","plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "es2021": true
  },
  "ignorePatterns": ["dist/**"]
}